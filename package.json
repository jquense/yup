{
  "name": "yup",
  "version": "0.22.0",
  "description": "Dead simple Object schema validation",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint src test --fix",
    "clean": "rimaf ./lib/*",
    "toc": "doctoc README.md --github",
    "build": "babel src --out-dir lib && npm run toc",
    "release": "release"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/yup.git"
  },
  "author": "@monasticpanic Jason Quense",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/yup/issues"
  },
  "homepage": "https://github.com/jquense/yup",
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "roots": [
      "test"
    ],
    "testRegex": "\\.js",
    "testPathIgnorePatterns": [
      "helpers\\.js"
    ],
    "transform": {
      "^.+\\.js$": "./transform-tests.js"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-jason": "^3.0.0",
    "benchmark": "^2.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "doctoc": "^1.0.0",
    "eslint": "^3.4.0",
    "eslint-config-jason": "^4.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "jest": "^20.0.3",
    "mt-changelog": "^0.6.2",
    "promises-aplus-tests": "^2.1.2",
    "release-script": "^0.5.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "case": "^1.2.1",
    "fn-name": "~1.0.1",
    "lodash": "^4.17.0",
    "property-expr": "^1.2.0",
    "toposort": "^0.2.10",
    "type-name": "^2.0.1"
  },
  "release-script": {
    "defaultDryRun": "false"
  }
}
