{
  "name": "jupp",
  "version": "0.22.0",
  "description": "Dead simple Object schema validation",
  "main": "lib/index.js",
  "scripts": {
    "benchmark": "babel-node benchmark/index.js",
    "jest": "jest",
    "test": "npm run lint && npm run jest",
    "tdd": "jest --watchAll",
    "lint": "eslint .",
    "clean": "rimaf ./lib/*",
    "toc": "doctoc README.md --github",
    "build": "babel src --out-dir lib && npm run toc",
    "release": "release"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ctavan/yup.git"
  },
  "author": "@monasticpanic Jason Quense",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctavan/yup/issues"
  },
  "homepage": "https://github.com/ctavan/yup",
  "jest": {
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "roots": [
      "test"
    ],
    "testRegex": "\\.js",
    "testPathIgnorePatterns": [
      "helpers\\.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "benchmark": "^2.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "doctoc": "^1.0.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.1.0",
    "promises-aplus-tests": "^2.1.2",
    "release-script": "^1.0.2",
    "sinon": "^3.3.0",
    "sinon-chai": "^2.13.0"
  },
  "dependencies": {
    "lodash": "^4.17.0",
    "property-expr": "^1.2.0",
    "synchronous-promise": "^1.0.17",
    "toposort": "^1.0.3",
    "type-name": "^2.0.1"
  },
  "release-script": {
    "defaultDryRun": "false"
  }
}
